@model UserModifyDataModel

@{
    ViewData["Title"] = "Edit User";
}

@section PageHeader{ 
<h4>Edit user</h4>
}

<div class="row">
    <div class="col-md-4">
        <form asp-route-id=@Model.Id method="post">
            <hr />

            @Html.EditorForModel()

            <button type="submit" class="btn btn-warning">Edit</button>
            <a class="btn btn-default" asp-area="Admin" asp-controller="Users" asp-action="Index">Cancel</a>
        </form>
    </div>
</div>



@section Scripts {

    @await Html.PartialAsync("_ValidationScriptsPartial")
}
