@model UserModifyDataModel

@{
    ViewData["Title"] = "Edit User";
}


<div class="row">
    <div class="col-md-4">
        <form asp-route-id=@Model.Id method="post">
            <h4>Edit user</h4>
            <hr />

            @Html.EditorForModel()

            <button type="submit" class="btn btn-default">Edit</button>
        </form>
    </div>
</div>



@section Scripts {

    @await Html.PartialAsync("_ValidationScriptsPartial")
}
